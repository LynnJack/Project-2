
<div class="itemcontainer">
<h2>Sign Up to Join the Community Share!</h2>

<%= form_for @user do |f| %>
  <%= f.label :name %> 
  <%= f.text_field :name %>

  <%= f.label :unit %> 
  <%= f.text_field :unit %>

  <%= f.label :email %> 
  <%= f.text_field :email %>

  <%= f.label :password %> 
  <%= f.password_field :password %>

  <%= f.submit "Sign Me Up!" %> 


<% end %>

  <%= link_to "Back to the Share List", items_path %>

</div>